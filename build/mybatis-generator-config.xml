<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- see http://mybatis.org/generator/configreference/xmlconfig.html -->
<generatorConfiguration>

    <context id="PostgresqlTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection
                connectionURL="jdbc:postgresql://127.0.0.1:5432/seezoon-stack"
                driverClass="org.postgresql.Driver"
                password=""
                userId="postgres">
            <!-- Only retrieve tables from the currently specified database-->
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>
        <!-- targetProject="MAVEN" is decided by mybatis-generator-maven-plugin,default is   ${project.build.directory}/generated-sources/mybatis-generator -->
        <javaModelGenerator targetPackage="com.seezoon.dao.modules.sys.entity"
                            targetProject="MAVEN">
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.seezoon.dao.modules.sys.dao"
                         targetProject="MAVEN">
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="com.seezoon.dao.modules.sys.dao"
                             targetProject="MAVEN"
                             type="XMLMAPPER">
        </javaClientGenerator>
        <table enableCountByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" enableUpdateByExample="false"
               tableName="%">
            <!-- Use for auto-increment primary key return -->
            <!--
            <generatedKey column="id" sqlStatement="JDBC"/>
            -->
        </table>
    </context>
</generatorConfiguration>